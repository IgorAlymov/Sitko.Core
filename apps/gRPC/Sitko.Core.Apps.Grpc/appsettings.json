{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Application": {
    "Web": {
      "Ports": {
        "Http": {
          "Port": 8080,
          "UseTLS": true
        },
        "gRPC": {
          "Port": 8081,
          "Protocol": "Http2",
          "UseTLS": true
        }
      }
    }
  }
}
